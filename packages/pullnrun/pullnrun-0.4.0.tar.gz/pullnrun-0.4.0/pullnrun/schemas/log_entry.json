{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"definitions": {
		"timestamp": {
			"oneOf": [
				{ "type": "number", "$comment": "timestamp in ms" },
				{ "type": "null" }
			]
		},
		"meta": {
			"type": "object",
			"properties": {
				"start": { "$ref": "#/definitions/timestamp"},
				"end": { "$ref": "#/definitions/timestamp"}
			},
			"required": ["start", "end"]
		}
	},
	"type": "object",
	"properties": {
		"type": { "type": "string" },
		"status": { "type": "string" },
		"data": { "type": "object" },
		"meta": { "$ref": "#/definitions/meta" },
		"errors": {
			"type": "array",
			"items": { "type": "string" }
		}
	},
	"required": ["type", "status", "data", "meta"]
}