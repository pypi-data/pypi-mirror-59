{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "required": [
    "content",
    "options"
  ],
  "properties": {
    "content": {
      "$id": "#/properties/content",
      "type": "object",
      "properties": {
        "key": {"type": "string"},
        "value": {"type": "string"},
      }
    },
    "options": {
      "$id": "#/properties/options",
      "type": "object",
      "required": [
        "reregister"
      ],
      "properties": {
        "reregister": {
          "$id": "#/properties/options/properties/reregister",
          "type": "boolean",
          "default": true
        }
      }
    }
  }
}