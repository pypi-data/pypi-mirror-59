#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'xled==0.6.0','console_scripts','xled'
__requires__ = 'xled==0.6.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('xled==0.6.0', 'console_scripts', 'xled')()
    )
