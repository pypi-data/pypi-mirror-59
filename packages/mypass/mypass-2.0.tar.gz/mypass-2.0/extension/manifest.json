{
  "name": "mypass",
  "description": "Browser integration for \"mypass\" password manager",
  "version": "1.10",
  "manifest_version": 2,
  "minimum_chrome_version": "35",
  "permissions": [
    "activeTab",
    "nativeMessaging",
    "contextMenus"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "19": "icon-19.png",
      "32": "icon-32.png",
      "38": "icon-38.png"
    },
    "default_popup": "popup.html",
    "title": "mypass",
    "browser_style": true
  },
  "applications": {
    "gecko": {
      "id": "mypass@snoack.addons.mozilla.org",
      "strict_min_version": "52.0"
    }
  },
  "key": ""
}
