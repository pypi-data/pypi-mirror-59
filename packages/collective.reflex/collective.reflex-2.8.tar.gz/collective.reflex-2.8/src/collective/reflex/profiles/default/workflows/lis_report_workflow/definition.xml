<?xml version="1.0"?>
<dc-workflow workflow_id="lis_report_workflow" title="LIMS Report Workflow" description=" - LIMS report workflow that is useful. - Things start out as analysis, has status [analysis | pending | reviewed | issued | published]." state_variable="report_state" initial_state="analysis" manager_bypass="False">
 <permission>Access contents information</permission>
 <permission>Change portal events</permission>
 <permission>Modify portal content</permission>
 <permission>View</permission>
 <state state_id="pending" title="Pending">
  <description>Report pending</description>
  <exit-transition transition_id="back-analysis"/>
  <exit-transition transition_id="review"/>
  <permission-map name="Access contents information" acquired="False">
   <permission-role>Contributor</permission-role>
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Reader</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="Change portal events" acquired="False">
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="Modify portal content" acquired="False">
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="View" acquired="False">
   <permission-role>Contributor</permission-role>
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Reader</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
 </state>
 <state state_id="published" title="Published">
  <description>Report published</description>
  <exit-transition transition_id="back-reviewed"/>
  <permission-map name="Access contents information" acquired="False">
   <permission-role>Contributor</permission-role>
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Reader</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="Change portal events" acquired="False">
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="Modify portal content" acquired="False">
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="View" acquired="False">
   <permission-role>Contributor</permission-role>
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Reader</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
 </state>
 <state state_id="reviewed" title="Reviewed">
  <description>Report reviewed</description>
  <exit-transition transition_id="issue"/>
  <exit-transition transition_id="publish"/>
  <exit-transition transition_id="back-pending"/>
  <permission-map name="Access contents information" acquired="False">
   <permission-role>Contributor</permission-role>
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Reader</permission-role>
   <permission-role>Reviewer</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="Change portal events" acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>Reviewer</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="Modify portal content" acquired="False">
   <permission-role>Manager</permission-role>
   <permission-role>Reviewer</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="View" acquired="False">
   <permission-role>Contributor</permission-role>
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Reader</permission-role>
   <permission-role>Reviewer</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
 </state>
 <state state_id="issued" title="Issued">
  <description>Report issued</description>
  <exit-transition transition_id="back-reviewed"/>
  <permission-map name="Access contents information" acquired="False">
   <permission-role>Contributor</permission-role>
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Reader</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="Change portal events" acquired="False">
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="Modify portal content" acquired="False">
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="View" acquired="False">
   <permission-role>Contributor</permission-role>
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Reader</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
 </state>
 <state state_id="analysis" title="Analysis">
  <description>Data analysis</description>
  <exit-transition transition_id="pend"/>
  <permission-map name="Access contents information" acquired="False">
   <permission-role>Contributor</permission-role>
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Reader</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="Change portal events" acquired="False">
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="Modify portal content" acquired="False">
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
  <permission-map name="View" acquired="False">
   <permission-role>Contributor</permission-role>
   <permission-role>Editor</permission-role>
   <permission-role>Manager</permission-role>
   <permission-role>Owner</permission-role>
   <permission-role>Reader</permission-role>
   <permission-role>Site Administrator</permission-role>
  </permission-map>
 </state>
 <transition transition_id="pend" title="Report pending" new_state="pending" trigger="USER" before_script="" after_script="">
  <description>Report pending</description>
  <action url="%(content_url)s/content_status_modify?workflow_action=pending" category="workflow" icon="">Report pending</action>
  <guard>
  </guard>
 </transition>
 <transition transition_id="review" title="Report review" new_state="reviewed" trigger="USER" before_script="" after_script="">
  <description>Report review</description>
  <action url="%(content_url)s/content_status_modify?workflow_action=review" category="workflow" icon="">Report review</action>
  <guard>
  </guard>
 </transition>
 <transition transition_id="publish" title="Report publish" new_state="published" trigger="USER" before_script="" after_script="">
  <description>Report publish</description>
  <action url="%(content_url)s/content_status_modify?workflow_action=published" category="workflow" icon="">Report publish</action>
  <guard>
  </guard>
 </transition>
 <transition transition_id="issue" title="Report issue" new_state="issued" trigger="USER" before_script="" after_script="">
  <description>Report issued. Report have been issued.</description>
  <action url="%(content_url)s/content_status_modify?workflow_action=issued" category="workflow" icon="">Report issue</action>
  <guard>
  </guard>
 </transition>
 <transition transition_id="back-analysis" title="Fallback analysis" new_state="analysis" trigger="USER" before_script="" after_script="">
  <description>Fallback to analysis</description>
  <action url="%(content_url)s/content_status_modify?workflow_action=back-analysis" category="workflow" icon="">Fallback analysis</action>
  <guard>
  </guard>
 </transition>
 <transition transition_id="back-pending" title="Fallback pending" new_state="pending" trigger="USER" before_script="" after_script="">
  <description>Fallback to pending.</description>
  <action url="%(content_url)s/content_status_modify?workflow_action=back-pending" category="workflow" icon="">Fallback pending</action>
  <guard>
  </guard>
 </transition>
 <transition transition_id="back-reviewed" title="Fallback reviewed" new_state="reviewed" trigger="USER" before_script="" after_script="">
  <description>Fallback to reviewed.</description>
  <action url="%(content_url)s/content_status_modify?workflow_action=back-reviewed" category="workflow" icon="">Fallback reviewed</action>
  <guard>
  </guard>
 </transition>
 <worklist worklist_id="reviewer_queue" title="">
  <description>Reviewer tasks</description>
  <action url="%(portal_url)s/search?report_state=pending" category="global" icon="">Pending (%(count)d)</action>
  <guard>
   <guard-permission>Review portal content</guard-permission>
  </guard>
  <match name="report_state" values="pending"/>
 </worklist>
 <variable variable_id="action" for_catalog="False" for_status="True" update_always="True">
  <description>Previous transition</description>
  <default>
   
   <expression>transition/getId|nothing</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="actor" for_catalog="False" for_status="True" update_always="True">
  <description>The ID of the user who performed the previous transition</description>
  <default>
   
   <expression>user/getId</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="comments" for_catalog="False" for_status="True" update_always="True">
  <description>Comment about the last transition</description>
  <default>
   
   <expression>python:state_change.kwargs.get('comment', '')</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="review_history" for_catalog="False" for_status="False" update_always="False">
  <description>Provides access to workflow history</description>
  <default>
   
   <expression>state_change/getHistory</expression>
  </default>
  <guard>
   <guard-permission>Request review</guard-permission>
   <guard-permission>Review portal content</guard-permission>
  </guard>
 </variable>
 <variable variable_id="time" for_catalog="False" for_status="True" update_always="True">
  <description>When the previous transition was performed</description>
  <default>
   
   <expression>state_change/getDateTime</expression>
  </default>
  <guard>
  </guard>
 </variable>
</dc-workflow>
